{% extends 'logConfigBase.yml.j2' %}
{% block service_name %}Service{% endblock %}

{% block categories %}
categories:
  default:
    appenders:
      - console
      - stdout
    level: {{ epg_log_level }}
  system: 
    appenders: 
      - system
      - stdout
    level: {{ epg_log_level }}
  access: 
    appenders: 
      - access
      - stdout
    level: {{ epg_log_level }}
  stream: 
    appenders: 
      - stream
      - stdout
    level: {{ epg_log_level }}
  encode:
    appenders:
      - encode
      - stdout
    level: {{ epg_log_level }}
{% endblock %}

